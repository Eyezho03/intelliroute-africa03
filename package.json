{
  "name": "intelliroute-africa",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "description": "AI-Powered Multi-Tier Supply Chain Platform for Africa",
  "author": "IntelliRoute Africa Team",
  "keywords": [
    "logistics",
    "supply-chain",
    "africa",
    "ai-powered",
    "route-optimization"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && node src/index.js",
    "build": "vite build",
    "build:pwa": "vite build && npm run generate-sw",
    "generate-sw": "echo 'Service Worker already included in public/sw.js'",
    "lint": "eslint .",
    "preview": "vite preview",
    "serve": "vite preview --host",
    "analyze": "vite build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@react-google-maps/api": "^2.20.7",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.11",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^7.7.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "tailwindcss": "^4.1.11",
    "usehooks-ts": "^3.1.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "terser": "^5.44.0",
    "vite": "^7.0.4"
  }
}
